{
  "name": "Reloaded Dark (core)",
  "colors": {
    "editor.background": "#1c1c1c",
    "editor.foreground": "#dfdfdf",
    "editor.selectionBackground": "#224467",
    "editor.selectionHighlightBackground": "#ADD6FF26",
    "editor.findMatchBackground": "#FF000050",
    "editor.hoverHighlightBackground": "#FFFFFF20",
    "diffEditor.insertedTextBackground": "#00FF0020",
    "diffEditor.removedTextBackground": "#FF000030"
  },
  "settings": [
    { "scope": ["emphasis"]                            , "settings": { "fontStyle": "italic"    } },
    { "scope": ["strong"]                              , "settings": { "fontStyle": "bold"      } },

    { "scope": ["markup.underline"]                    , "settings": { "fontStyle": "underline" } },
    { "scope": ["markup.bold"]                         , "settings": { "fontStyle": "bold"      } },
    { "scope": ["markup.heading"]                      , "settings": { "foreground": "#6796e6"  } },
    { "scope": ["markup.italic"]                       , "settings": { "fontStyle": "italic"    } },
    { "scope": ["markup.inserted"]                     , "settings": { "foreground": "#b5cea8"  } },
    { "scope": ["markup.deleted"]                      , "settings": { "foreground": "#ce9178"  } },
    { "scope": ["markup.changed"]                      , "settings": { "foreground": "#569cd6"  } },

    { "scope": ["header"]                              , "settings": { "foreground": "#000080"  } },
    { "scope": ["invalid"]                             , "settings": { "foreground": "#f44747"  } },
    { "scope": ["meta.invalid-continuation.cpp"]       , "settings": { "foreground": "#f44747", "background": "#8F0000"  } },

    { "scope": ["comment"]                             , "settings": { "foreground": "#888888"  } },
    { "scope": ["comment.line.banner"]                 , "settings": { "fontStyle" : "bold"     } },
    { "scope": ["comment.code-block",
                "comment.code-punctuation"]            , "settings": { "foreground": "#afafaf"  } },
    { "scope": ["comment.docgroup", "comment.command"] , "settings": { "foreground": "#bf8e35"  } },
    { "scope": ["comment.variable"]                    , "settings": { "foreground": "#688ed7"  } },
    { "scope": ["comment.section"]                     , "settings": { "foreground": "#909090", "fontStyle": "bold" } },
    { "scope": ["comment.decoration"]                  , "settings": { "foreground": "#909090"  } },
    { "scope": ["comment.note"]                        , "settings": { "foreground": "#A0A0A0", "fontStyle": "bold" } },

    { "scope": ["keyword"]                             , "settings": { "foreground": "#569cd6"  } },

    { "scope": ["constant.boolean"]                    , "settings": { "foreground": "#569cd6"  } },
    { "scope": ["constant.numeric"]                    , "settings": { "foreground": "#78cf8a"  } },
    { "scope": ["constant.number-suffix"]              , "settings": { "foreground": "#478a55"  } },
    { "scope": ["constant.regexp"]                     , "settings": { "foreground": "#b46695"  } },
    { "scope": ["constant.rgb-value"]                  , "settings": { "foreground": "#d4d4d4"  } },

    { "scope": ["entity.name.tag"]                     , "settings": { "foreground": "#569cd6"  } },
    { "scope": ["entity.name.tag.css"]                 , "settings": { "foreground": "#569cd6", "fontStyle": "bold" } },
    { "scope": ["entity.name.selector"]                , "settings": { "foreground": "#d7ba7d"  } },
    { "scope": ["entity.other.attribute-name.id"]      , "settings": { "foreground": "#f37171"  } },
    { "scope": ["entity.other.attribute-name.class"]   , "settings": { "foreground": "#5ec0de"  } },
    { "scope": ["entity.other.attribute-name.pseudo-class"], "settings": { "foreground": "#bf6be2", "fontStyle": "bold"  } },

    { "scope": ["meta.tag"]                            , "settings": { "foreground": "#808080"  } },
    { "scope": ["meta.selector"]                       , "settings": { "foreground": "#d7ba7d"  } },
    { "scope": ["meta.property-name"]                  , "settings": { "foreground": "#b7a87d"  } },
    { "scope": ["meta.template.expression.js"]         , "settings": { "foreground": "#f1cc7a"  } },

    { "scope": ["support.assert.cpp"]                  , "settings": { "foreground": "#0077ca", "fontStyle": "bold"  } },
    { "scope": ["support.asm.cpp",
                "support.likely-unlikely.cpp"]         , "settings": { "foreground": "#b64ad6", "fontStyle": "bold"  } },
    { "scope": ["support.constant.success.libc.cpp",
                "support.constant.success.misc.cpp",
                "support.function-name.success.cpp"]   , "settings": { "foreground": "#85E775" } },
    { "scope": ["support.constant.signal.cpp",
                "support.constant.failure.libc.cpp",
                "support.constant.failure.misc.cpp",
                "support.function-name.failure.cpp"]   , "settings": { "foreground": "#ff6868" } },

    { "scope": ["support.variable.libc.cpp",
                "support.function-name.std.cpp"]       , "settings": { "foreground": "#fa7d4a"  } },

    { "scope": ["support.all-upper"]                   , "settings": { "foreground": "#2d9ae6", "fontStyle": "bold" } },
    { "scope": ["support.class.builtin",
                "support.first-upper.cpp"]             , "settings": { "foreground": "#1ad1ff"  } },
    { "scope": ["entity.name.type.class.python"]       , "settings": { "foreground": "#1ad1ff", "fontStyle": "bold" } },

    { "scope": ["punctuation.definition.variable.shell"], "settings": { "foreground": "#eeeeee", "fontStyle": "bold" } },
    { "scope": ["variable.other.bracket.shell"]         , "settings": { "foreground": "#3db4ff", "fontStyle": "bold" } },

    { "scope": ["storage",
                "storage.type",
                "storage.type.access",
                "storage.modifier.const",
                "storage.modifier.shell",
                "constant.language",
                "constant.nullptr.cpp",
                "keyword.control",
                "keyword.operator.noexcept.cpp",
                "keyword.operator.sizeof.cpp",
                "keyword.operator.in.js",
                "keyword.operator.typeof.js",
                "support.something-t.cpp",
                "support.type.misc.cpp",
                "support.type.libc.cpp",
                "support.type.python"]                 , "settings": { "foreground": "#2d9ae6"  } },

    { "scope": ["meta.preprocessor.command.cpp",
                "meta.preprocessor.keyword.cpp",
                "meta.preprocessor.hash.cpp",
                "meta.preprocessor.keyword"]           , "settings": { "foreground": "#5d94dc", "fontStyle": "bold" } },

    { "scope": ["meta.line-continuation.cpp"]          , "settings": { "foreground": "#EDC869", "fontStyle": "bold" } },

    { "scope": ["storage.modifier"]                    , "settings": { "foreground": "#869eb7"  } },
    { "scope": ["keyword.operator.vaargs",
                "storage.modifier.js",
                "storage.modifier.static.cpp",
                "storage.modifier.attribute.cpp",
                "storage.modifier.inline.cpp",
                "storage.modifier.extern.cpp",
                "storage.modifier.explicit.cpp",
                "storage.modifier.callconv.cpp",
                "storage.modifier.final.cpp",
                "storage.modifier.virtual.cpp",
                "storage.modifier.override.cpp",
                "storage.modifier.mutable.cpp",
                "storage.modifier.volatile.cpp",
                "storage.modifier.restrict.cpp"]       , "settings": { "foreground": "#2d9ae6", "fontStyle": "bold" } },

    { "scope": ["entity.name.function.python"]         , "settings": { "foreground": "#ffffff", "fontStyle": "bold" } },

    { "scope": ["operator.namespace-separator.cpp",
                "punctuation.section.function.begin.python",
                "punctuation.separator.colon.python"]  , "settings": { "foreground": "#fa8d6a", "fontStyle": "bold" } },

    { "scope": ["support.constant.std.libc.cpp",
                "support.constant.predefined.cpp",
                "support.constant.k-something.cpp"]    , "settings": { "foreground": "#ea6ce5" } },

    { "scope": ["string",
                "string.tag",
                "punctuation.single.quoted.string",
                "punctuation.double.quoted.string"]    , "settings": { "foreground": "#8f9d6a"  } },
    { "scope": ["constant.character.escape"]           , "settings": { "foreground": "#EDC869"  } },
    { "scope": ["constant.character.format"]           , "settings": { "foreground": "#AfFd8a"  } },
    { "scope": ["string.regexp"]                       , "settings": { "foreground": "#d16969"  } },
    { "scope": ["string.interpolated.backtick.shell"]  , "settings": { "foreground": "#ff8600"  } },

    { "scope": ["support.property-name",
                "support.type.property-name.json",
                "entity.other.attribute-name.html",
                "meta.structure.dictionary.key.python",
                "meta.object-literal.key"]             , "settings": { "foreground": "#cda869"  } },

    { "scope": ["variable.language.this",
                "variable.language.special.self.python",
                "variable.parameter.function.language.special.self.python",
                "entity.name.function.decorator.python",
                "support.first-lower-underscore-prefix.cpp",
                "support.first-lower-underscore-suffix.cpp"], "settings": { "foreground": "#e6ba80"  } },

    { "scope": ["support.double-underscore-prefix.cpp",
                "support.function.magic.python",
                "support.function.builtin.shell"]      , "settings": { "foreground": "#569cd6"  } },

    { "scope": ["keyword.control.less"]                , "settings": { "foreground": "#d7ba7d"  } },
    { "scope": ["keyword.operator",
                "keyword.operator.cast.cpp"]           , "settings": { "foreground": "#fa8d6a"  } },
    { "scope": ["keyword.operator.new.cpp",
                "keyword.operator.delete.cpp"]         , "settings": { "foreground": "#569cd6"  } },
    { "scope": ["keyword.other.unit"]                  , "settings": { "foreground": "#478a55"  } },
    { "scope": ["support.function.git-rebase"]         , "settings": { "foreground": "#d4d4d4"  } },
    { "scope": ["constant.sha.git-rebase"]             , "settings": { "foreground": "#b5cea8"  } },
    { "scope": ["storage.modifier.import.java",
                "storage.modifier.package.java"]       , "settings": { "foreground": "#d4d4d4"  } }
  ]
}
